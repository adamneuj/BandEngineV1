@model IEnumerable<BandEngine.Models.Email>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Add Email", "Create")
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Email Address")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EmailAddress)
            </td>
            <td>
                @Html.ActionLink("Delete", "Delete", new { id = item.EmailId })
            </td>
        </tr>
    }

</table>
